{
  "_version": "1.49.0",
  "sap.app": {
    "id": "nba",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.10.3",
      "toolsId": "9701f15e-2812-437f-8e6a-71ddbe8d1471"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZGW_NBA_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.116.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.ui.webc.main": {},
        "sap.suite.ui.microchart": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "nba.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "nba.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteEquipos",
          "pattern": "",
          "target": [
            "TargetEquipos"
          ]
        },
        {
          "name": "RouteJugadores",
          "pattern": "jugadores/{equipoId}",
          "target": [
            "TargetJugadores"
          ]
        },
        {
          "name": "RouteDetalle",
          "pattern": "detalle/{jugadorId}",
          "target": [
            "TargetDetalle"
          ]
        }
      ],
      "targets": {
        "TargetEquipos": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Equipos",
          "viewName": "Equipos"
        },
        "TargetJugadores": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Jugadores",
          "viewName": "Jugadores"
        },
        "TargetDetalle": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Detalle",
          "viewName": "Detalle"
        }
      }
    },
    "rootView": {
      "viewName": "nba.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
